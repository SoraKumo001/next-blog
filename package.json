{
  "name": "next-blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint:fix": "next lint --fix && prettier -w src",
    "storybook": "storybook dev -p 9001",
    "sf": "scaffold create",
    "docker:start": "docker compose -p next-blog -f docker/docker-compose.yml up -d",
    "docker:build": "docker compose -p next-blog -f docker/docker-compose.yml build --parallel --compress --pull",
    "docker:upload": "docker save  next-blog-next-blog | env-exec -e .env-deploy docker load",
    "deploy": "yarn docker:build && yarn docker:upload && env-exec -e .env-deploy -- docker compose -p next-blog -f docker/docker-compose.yml up -d",
    "remote-start": "env-exec -e .env-deploy -- docker compose -p next-blog -f docker/docker-compose.yml up",
    "set-cors": "env-exec -e .env -- gsutil cors set cors.json gs://$(NEXT_PUBLIC_storageBucket)"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.16.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@mui/styled-engine": "^5.11.0",
    "@mui/styles": "^5.11.2",
    "@mui/system": "^5.11.5",
    "@node-libraries/wasm-webp-encoder": "^1.0.5",
    "@react-libraries/markdown-editor": "^1.0.21",
    "@react-libraries/use-local-event": "^1.0.2",
    "@react-libraries/use-local-state": "^1.0.12",
    "@react-libraries/use-ssr": "^1.0.17",
    "@react-libraries/virtual-window": "^1.0.8",
    "@vercel/og": "^0.0.27",
    "buffer": "^6.0.3",
    "date-fns": "^2.29.3",
    "firebase": "^9.16.0",
    "next": "13.1.4",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-scroll": "^1.8.9",
    "react-syntax-highlighter": "^15.5.0",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0",
    "worker-lib": "^1.0.1"
  },
